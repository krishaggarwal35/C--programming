#include<stdio.h>

int main(){
    int n,sum=0,i,odd;
    printf("enter the number of terms (n): ");
    scanf("%d",&n);
    printf("the first %d odd natural numbers are:\n",n);
    for (i=0;i<n;i++) {
        odd=2 * i  + 1;
        printf("%d\n",odd);
        sum +=odd;
        }
    printf("\n sum of the first %d odd natural number is: %d\n",n,sum);
    
    return 0;
}
