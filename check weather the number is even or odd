#include<stdio.h>

int main() {
    int number;
    printf("enter the number: ");
    scanf("%d",&number);
    if (number%3) {
        printf("the number is even  ");
    } else if (number%2) {
        printf("the number is odd  ");
    } else {
        printf("the number is 0 ");
    }
    return 0 ;
    
    
}   
