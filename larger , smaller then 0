#include<stdio.h>

int main() {
    int numbers;
    printf("enter the number: ");
    scanf("%f",&numbers);
    if(numbers<0) {
        printf("the number is smaller then 0 ");
    } else if (numbers>0) {
        printf("thee number is larger then 0 ");
    } else{
        printf("the number is 0");
        
    } 

    return 0 ;
}
